ty I64
fn zero = 0;

ty I64
fn one = 1;

ty I64 -> I64
fn id x = x;

ty I64
fn nestedIdOne = id (id one);

ty I64 -> I64 -> I64
fn addValues x y = x + y;

ty I64 -> I64 -> I64
fn subValues x y = x - y;

ty I64 -> I64 -> I64
fn mulValues x y = x * y;

ty I64 -> I64 -> I64
fn divValues x y = x / y;

ty I64 -> I64 -> I64 -> I64 -> I64
fn addMulValues x y z w = x * y + z * w;

ty I64 -> I64 -> I64
fn pipelineAddValues x y = id <| x + y
